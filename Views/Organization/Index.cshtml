@model List<OrganizationModel>
@{
    ViewData["Title"] = "Oganization";
    Layout = "~/Views/Shared/_Layout_Dashboard.cshtml";
}
<script>
    document.getElementById("HeaderTopic").innerHTML = "Oganization";
</script>
<script src="~/js/Organized/org.js"></script>
<div id="chartDiv" style="width: 100%; height: 600px; margin: 0px auto; overflow: hidden"></div>

<p>
    <a asp-action="Create">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model[0].name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].position)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].photo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].parent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].work_quality)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].initiative)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].cooperative)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.position)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.photo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.parent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.work_quality)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.initiative)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cooperative)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a>
                @*<a asp-action="Delete" asp-route-id="@item.id">Delete</a>*@
            </td>
        </tr>
        }
    </tbody>
</table>



