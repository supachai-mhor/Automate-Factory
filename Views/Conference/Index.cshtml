}

<script src="~/js/signalr/dist/browser/signalr.js"></script>

<script src="~/js/Conference/AddMachine.js"></script>
<script>

var currentChatWithName = "";

document.getElementById("HeaderTopic").innerHTML = "Message";

$(document).ready(function () {

    $('#action_menu_btn').click(function () {
        $('.action_menu').toggle();
    });

    hideChatMessagePage();
    hideAddMachinePage();
});

    var loginToken = "@ViewBag.token";

var myImg = "https://ptetutorials.com/images/user-profile.png";

var connection = new signalR.HubConnectionBuilder()
    .withUrl("/AutomateHub", { accessTokenFactory: () => loginToken })
    .withAutomaticReconnect()
    .build();

connection.on("ReceiveUserImage", (img) => OnReceiveUserImage(img));
connection.on("ReceiveUserOnline", (user) => OnReceiveUserOnline(user));
connection.on("ReceiveUserOffline", (user) => OnReceiveUserOffline(user));
connection.on("ReceiveSearchMachine", (machine) => OnReceiveSearchMachine(machine));
connection.on("ReceiveRelationResult", (result) => OnReceiveRelationResult(result));
connection.on("ReceiveAddMachineToContactsList", (machines) => OnReceiveAddMachineToContactsList(machines));
connection.on("ReceiveMessageConference", (user, data, mcImg) => OnReceiveMessageConference(user, data, mcImg));
connection.on("ReceiveLoadMessageToUser", (data,email) => OnReceiveLoadMessageToUser(data,email));

connection.start().then(function () {

        getMachineContactsList();
        getOnlineAllUser();


    }).catch(function (err) {
    return console.error(err.toString());
});
function OnReceiveUserImage(img) {
    if (img != "" && img !=null) {
        myImg = img
    } 
}
function OnReceiveUserOnline(user) {

    var allMessageList = document.getElementById("contactListId");
    const liList = allMessageList.querySelectorAll('li');

    for (var i = 0, iLen = liList.length; i < iLen; i++) {
        var getUserInfo = liList[i].getElementsByClassName("user_info");
        var getUserNameList = getUserInfo[0].getElementsByTagName("span");
        var getUserName = getUserNameList[0].innerHTML;

        if (user == getUserName) {
            var getImgConstList = liList[i].getElementsByClassName("img_cont");
            var getOnlineClass = getImgConstList[0].querySelector('span');
            //var setOnlineText = getUserInfo[0].getElementsByTagName("p");
            //setOnlineText[0].innerHTML = user + " is online";
            getOnlineClass.className = "online_icon";

            var getNameCurrentChat = document.getElementById("nameChatWithMessage").innerHTML;
            if (getNameCurrentChat == 'Chat with ' + user) {
                document.getElementById("onlineChatMessageStatus").className = "online_icon";
            }
        }
    }

}
function OnReceiveUserOffline(user) {
    var allMessageList = document.getElementById("contactListId");
    const liList = allMessageList.querySelectorAll('li');

    for (var i = 0, iLen = liList.length; i < iLen; i++) {
        var getUserInfo = liList[i].getElementsByClassName("user_info");
        var getUserNameList = getUserInfo[0].getElementsByTagName("span");
        var getUserName = getUserNameList[0].innerHTML;

        if (user == getUserName) {
            var getImgConstList = liList[i].getElementsByClassName("img_cont");
            var getOnlineClass = getImgConstList[0].querySelector('span');
            //var setOnlineText = getUserInfo[0].getElementsByTagName("p");
            //setOnlineText[0].innerHTML = user + " is offline";
            getOnlineClass.className = "online_icon offline";

            var getNameCurrentChat = document.getElementById("nameChatWithMessage").innerHTML;
            if (getNameCurrentChat == 'Chat with ' + user) {
                document.getElementById("onlineChatMessageStatus").className = "online_icon offline";
            }
        }
    }
    }
function OnReceiveSearchMachine(machine) {

    if (machine == "") {
        // search result
        var getDIV_AddMachineResult = document.getElementById("AddMachineResult");
        while (getDIV_AddMachineResult.firstChild) {
            getDIV_AddMachineResult.removeChild(getDIV_AddMachineResult.lastChild);
        }
        //search result detail
        var getDIV_AddMachineResultDetail = document.getElementById("AddMachineResultDetail");
        while (getDIV_AddMachineResultDetail.firstChild) {
            getDIV_AddMachineResultDetail.removeChild(getDIV_AddMachineResultDetail.lastChild);
        }

        const divInfo = document.createElement('div');
        divInfo.className = 'user_info';
        divInfo.innerHTML = '<span>  NOT FOUND MACHINE </span>';
        getDIV_AddMachineResult.appendChild(divInfo);
    }
    else {
        var myMachine = JSON.parse(machine);
        if (myMachine != undefined) {

            // search result
            var getDIV_AddMachineResult = document.getElementById("AddMachineResult");
            while (getDIV_AddMachineResult.firstChild) {
                getDIV_AddMachineResult.removeChild(getDIV_AddMachineResult.lastChild);
            }
            const divImage = document.createElement('div');
            divImage.className = 'img_cont';
            divImage.innerHTML = '<img src="' + myMachine.machineImage + '" class="img-thumbnail user_img">';
            const divInfo = document.createElement('div');
            divInfo.className = 'user_info';
            divInfo.innerHTML = '<span>' + myMachine.name + '</span>' +
                '<input type="button" class="btn btn-success" value="ADD" style="right:20px;position:absolute;" onclick="AddRelationWithMachine()" />';

            getDIV_AddMachineResult.appendChild(divImage);
            getDIV_AddMachineResult.appendChild(divInfo);

            //search result detail
            var getDIV_AddMachineResultDetail = document.getElementById("AddMachineResultDetail");
            while (getDIV_AddMachineResultDetail.firstChild) {
                getDIV_AddMachineResultDetail.removeChild(getDIV_AddMachineResultDetail.lastChild);
            }
            const divDetail = document.createElement('div');
            divDetail.innerHTML = '<dl class="card-link row col-sm-12">' +

                '<dt class="col-sm-4">Plant:</dt><dd class="col-sm-8">' + myMachine.plant + '</dd>' +
                '<dt class="col-sm-4">Process:</dt><dd class="col-sm-8">' + myMachine.process + '</dd>' +
                '<dt class="col-sm-4">Line:</dt><dd class="col-sm-8">' + myMachine.line + '</dd>' +
                '<dt class="col-sm-4">Vendor:</dt><dd class="col-sm-8">' + myMachine.vendor + '</dd>' +
                '<dt class="col-sm-4">Supervisor:</dt><dd class="col-sm-8">' + myMachine.supervisor + '</dd>' +
                '<dt class="col-sm-4">Installed Date:</dt><dd class="col-sm-8">' + myMachine.installed_date + '</dd>' +
                '<dt class="col-sm-4">Description:</dt><dd class="col-sm-8">' + myMachine.description + '</dd>' +
                '</dl>';

            getDIV_AddMachineResultDetail.appendChild(divDetail);

        }
    }
   
}
function OnReceiveRelationResult(result) {

    if (result == "error") {
        // search result
        var getDIV_AddMachineResult = document.getElementById("AddMachineResult");
        while (getDIV_AddMachineResult.firstChild) {
            getDIV_AddMachineResult.removeChild(getDIV_AddMachineResult.lastChild);
        }
        //search result detail
        var getDIV_AddMachineResultDetail = document.getElementById("AddMachineResultDetail");
        while (getDIV_AddMachineResultDetail.firstChild) {
            getDIV_AddMachineResultDetail.removeChild(getDIV_AddMachineResultDetail.lastChild);
        }

        const divInfo = document.createElement('div');
        divInfo.className = 'user_info';
        divInfo.innerHTML = '<span>  Can not add this machine </span>';
        getDIV_AddMachineResult.appendChild(divInfo);
    }
    else if (result == "has") {
        
        // search result
        var getDIV_AddMachineResult = document.getElementById("AddMachineResult");
        while (getDIV_AddMachineResult.firstChild) {
            getDIV_AddMachineResult.removeChild(getDIV_AddMachineResult.lastChild);
        }
        //search result detail
        var getDIV_AddMachineResultDetail = document.getElementById("AddMachineResultDetail");
        while (getDIV_AddMachineResultDetail.firstChild) {
            getDIV_AddMachineResultDetail.removeChild(getDIV_AddMachineResultDetail.lastChild);
        }

        const divInfo = document.createElement('div');
        divInfo.className = 'user_info';
        divInfo.innerHTML = '<span>  Already has in your Contacts  </span>';
        getDIV_AddMachineResult.appendChild(divInfo);
    }
    else {
        var myMachine = JSON.parse(result);
        if (myMachine != null) {
            // search result
            var getDIV_AddMachineContact = document.getElementById("contactListId");
            const liData = document.createElement('li');
            liData.innerHTML = '<div class="d-flex bd-highlight">' +
                '<div class="img_cont"><img src="' + myMachine.machineImage + '" class="rounded-circle user_img"><span class="online_icon offline"></span></div>' +
                '<div class="user_info"><span>' + myMachine.name + '</span></div>' +
                '</div>';

            getDIV_AddMachineContact.appendChild(liData);
            document.getElementById("SearchAddMachine").value = "";
        }
        
    }
    }
function OnReceiveAddMachineToContactsList(machines) {

    var myMachine = JSON.parse(machines);
    if (myMachine != null) {
        for (i = 0; i < myMachine.length; i++) {
            // search result
            var getDIV_AddMachineContact = document.getElementById("contactListId");
            const liData = document.createElement('li');
            liData.innerHTML = '<div class="d-flex bd-highlight">' +
                    '<div class="img_cont"><img src="' + myMachine[i].machineImage + '" class="rounded-circle user_img"><span class="online_icon offline"></span></div>' +
                '<div class="user_info"><span>' + myMachine[i].name + '</span></div>' +
                '</div>';

            getDIV_AddMachineContact.appendChild(liData);
            document.getElementById("SearchAddMachine").value = "";
            getOnlineAllUser();
        }
    }
}
function OnReceiveMessageConference(user, data,mcImg) {

    //console.log(user + ":" + data);
    //console.log(currentChatWithName);
    if (currentChatWithName == user) {
        ReceivedMessage(data, mcImg);
    }
    else {


        AddBadgesSpan(1);


        var allMessageList = document.getElementById("contactListId");
        const liList = allMessageList.querySelectorAll('li');


        for (var i = 0, iLen = 1; i < iLen; i++) {

            var getUserInfo = liList[i].getElementsByClassName("user_info");
            var getUserNameList = getUserInfo[0].getElementsByTagName("span");
            var getUserName = getUserNameList[0].innerHTML;

            if (user == getUserName) {

                var getCurrentSpan = getUserInfo[0].getElementsByClassName("badge");

                if (getCurrentSpan.length > 0) {
                    var countcurrent = Number(getCurrentSpan[0].innerHTML) +1 ;
                    getCurrentSpan[0].innerHTML = countcurrent;
                }
                else {
                    var newSpan = document.createElement("SPAN");
                    newSpan.className = "badge badge-danger";
                    newSpan.style = "right:30px; position:absolute;"
                    newSpan.innerHTML = 1;
                    var getBefore = getUserInfo[0].getElementsByTagName("p");
                    getUserInfo[0].insertBefore(newSpan, getBefore[0]);
                }
            }
		}
    }
    }

function CheckLastTimeFotmatMessage(time) {

    var allMessageList = document.getElementById("divMessageList");
    var queryCurrentDate = allMessageList.getElementsByClassName('msg_time_header');
    if (queryCurrentDate.length > 0) {
        if (queryCurrentDate[queryCurrentDate.length - 1].innerText != time) {

            const div = document.createElement('div');
            div.className = 'd-flex justify-content-center';
            div.innerHTML = '<div class="msg_time_header"><span> ' + time + ' </span></div>';
            allMessageList.appendChild(div);

        }

    } else {
        const div = document.createElement('div');
        div.className = 'd-flex justify-content-center';
        div.innerHTML = '<div class="msg_time_header"><span> ' + time + ' </span></div>';
        allMessageList.appendChild(div);
    }
}

function ReceivedMessage(text, img) {

    WriteTodayMessage();

    if (text != "") {
        text = text.replace(/\n/g, "<br>");
        var splitText = text.split('<br>');
        if (splitText.length > 0) {
            for (t = 0; t < splitText.length; t++) {

                if (splitText[t].length > 30) {
                    checkTextLength = true;
                    break;
                }
            }
        }
        var allMessageList = document.getElementById("divMessageList");

        const div = document.createElement('div');
        var d = new Date();
        var nowTime = d.toLocaleString('en-US', { hour: 'numeric', minute: 'numeric', hour12: true });

        div.className = 'd-flex justify-content-start mb-4';

        if (text.length > 30 && checkTextLength == true) {

            div.innerHTML = '<div class="img_cont_msg"><img src="' + img + '" class="rounded-circle user_img_msg"></div>' +
                '<div class="msg_cotainer" style="width: 50%;">' + text + '<span class="msg_time">' + nowTime + '</span></div>';
        }
        else {
            div.innerHTML = '<div class="img_cont_msg"><img src="' + img + '" class="rounded-circle user_img_msg"></div>' +
                '<div class="msg_cotainer" >' + text + '<span class="msg_time">' + nowTime + '</span></div>';
        }

        allMessageList.appendChild(div);
    }
}
function SearchMachine() {
    if (document.getElementById("SearchRadioAddMachineHashID").checked) {
        connection.invoke("SendSearchMachine", document.getElementById("SearchAddMachine").value,true).catch(function (err) {
        return console.error(err.toString());
        });
    }
    else {
        connection.invoke("SendSearchMachine", document.getElementById("SearchAddMachine").value,false).catch(function (err) {
            return console.error(err.toString());
        });
    }
   
}
function AddRelationWithMachine() {
    if (document.getElementById("SearchRadioAddMachineHashID").checked) {
        connection.invoke("SendAddRelationWithMachine", document.getElementById("SearchAddMachine").value, true).catch(function (err) {
            return console.error(err.toString());
        });
    }
    else {
        connection.invoke("SendAddRelationWithMachine", document.getElementById("SearchAddMachine").value, false).catch(function (err) {
            return console.error(err.toString());
        });
    }

}   
function getOnlineAllUser() {

        connection.invoke("SendGetOnlineAllUser").catch(function (err) {
            return console.error(err.toString());
        });
}
function getMachineContactsList() {

        connection.invoke("SendGetMachineContactsList").catch(function (err) {
            return console.error(err.toString());
        });
    }


function SentMessageToHub() {

    var text = document.getElementById("SendTextArea").value;
    if (text != "") {

        SendMessage();
        var removeActiveId = document.getElementById("contactListId");
        var queryActive = removeActiveId.getElementsByClassName('active');

        connection.invoke("SendMessageToMachine", text, queryActive[0].innerText).catch(function (err) {
            return console.error(err.toString());
        });
    }
    }
function SendMessage() {

    WriteTodayMessage();

    var allMessageList = document.getElementById("divMessageList");

    var text = document.getElementById("SendTextArea").value;
    if (text != "") {

        text = text.replace(/\n/g, "<br>");
        const div = document.createElement('div');
        var d = new Date();
        var nowTime = d.toLocaleString('en-US', { hour: 'numeric', minute: 'numeric', hour12: true });
        div.className = 'd-flex justify-content-end mb-4';

        if (text.length > 30) {
            div.innerHTML = '<div class="msg_cotainer_send" style="width: 50%;" >' + text + '<span class="msg_time_send">' + nowTime + '</span></div>' +
                '<div class="img_cont_msg"><img src="' + myImg + '" class="rounded-circle user_img_msg"></div>';

        }
        else {
            div.innerHTML = '<div class="msg_cotainer_send" >' + text + '<span class="msg_time_send">' + nowTime + '</span></div>' +
                '<div class="img_cont_msg"><img src="' + myImg + '" class="rounded-circle user_img_msg"></div>';
        }

        allMessageList.appendChild(div);
        document.getElementById("SendTextArea").value = "";
    }
}

function LoadMessage(machineName) {
    connection.invoke("SendLoadMessageByUser", machineName).catch(function (err) {
    return console.error(err.toString());
    });
}

function SendClearMessageToHub() {
    var removeActiveId = document.getElementById("contactListId");
    var queryActive = removeActiveId.getElementsByClassName('active');

    connection.invoke("SendClearMessageByUser",queryActive[0].innerText).catch(function (err) {
        return console.error(err.toString());
    });

    ClearMessage();
}

function WriteTodayMessage() {

    var allMessageList = document.getElementById("divMessageList");

    var queryCurrentDate = allMessageList.getElementsByClassName('msg_time_header');
    if (queryCurrentDate.length > 0) {
        if (queryCurrentDate[queryCurrentDate.length - 1].innerText != "Today") {
            const div = document.createElement('div');
            div.className = 'd-flex justify-content-center';
            div.innerHTML = '<div class="msg_time_header"><span> Today </span></div>';
            allMessageList.appendChild(div);
        }
    } else {
        const div = document.createElement('div');
        div.className = 'd-flex justify-content-center';
        div.innerHTML = '<div class="msg_time_header"><span> Today </span></div>';
        allMessageList.appendChild(div);
    }
    
    }

function ClearMessage() {

    var allMessageList = document.getElementById("divMessageList");
    var getClearDisplay = document.getElementById("clearMessageDisplay");
    getClearDisplay.style = "display:none;";
    //e.firstElementChild can be used.
    var child = allMessageList.lastElementChild;
    while (child) {
        allMessageList.removeChild(child);
        child = allMessageList.lastElementChild;
    }
}
function showChatMessagePage() {

    hideAddMachinePage();

    var divToShow = $('div#ChatMessagePage');
    divToShow.show();

}
function showChatListPage() {

    hideAddMachinePage();
    hideChatMessagePage();

    var divToShow = $('div#ChatListPage');
    divToShow.show();

}
function showAddMachinePage() {

    hideChatMessagePage();

    var divToShow = $('div#AddMachinePage');
    divToShow.show();

}
function hideChatMessagePage() {
    var divToHide = $('div#ChatMessagePage');
    divToHide.hide();
    currentChatWithName = "";
}
function hideAddMachinePage() {
    var divToHide = $('div#AddMachinePage');
    divToHide.hide();
}

function getEventTarget(e) {
    e = e || window.event;
    return e.target || e.srcElement;
}
function onclickUlContact(event) {

    ClearMessage();
   
    showChatMessagePage();

    var target = getEventTarget(event);
    var indexLi = target.parentElement.closest("li");
    if (indexLi != null) {

        //remove and add active list
        var removeActiveId = document.getElementById("contactListId");
        var queryActive = removeActiveId.getElementsByClassName('active');
        if (queryActive.length > 0) {
            queryActive[0].classList.remove('active');
        }
        indexLi.classList.add('active');


        // remove badge
        var getImgConstList = indexLi.getElementsByClassName("img_cont");
        var getCurrentSpan = indexLi.getElementsByClassName("badge");

        if (getCurrentSpan.length > 0) {

            var deleteCountAll = Number(getCurrentSpan[0].innerHTML);

            var conferenceMenu = document.getElementById("ConferenceMenu");
            var getCurrentSpanAll = conferenceMenu.getElementsByClassName("badge");

            if (getCurrentSpanAll.length > 0) {

                var countCurrentAllBadge = Number(getCurrentSpanAll[0].innerHTML);
                countCurrentAllBadge -= deleteCountAll;

                if (countCurrentAllBadge == 0) {
                        getCurrentSpanAll[0].parentElement.removeChild(getCurrentSpanAll[0]);
                }
                else {
                    getCurrentSpanAll[0].innerHTML = countCurrentAllBadge;
                }

            }
            getCurrentSpan[0].parentElement.removeChild(getCurrentSpan[0]);
        }


        //console.log(getImgConstList);

        const img = getImgConstList[0].querySelector('img');
        var imgpath = img.getAttribute('src');
        //console.log(imgpath);

        var getOnlineClass = getImgConstList[0].querySelector('span');
        var classOnlineName = getOnlineClass.className ;
        //console.log(classOnlineName);

        var getUserInfo = indexLi.getElementsByClassName("user_info");
        var getUserNameList = getUserInfo[0].getElementsByTagName("span");
        var getUserName = getUserNameList[0].innerHTML;
        //console.log(getUserName);

        document.getElementById("nameChatWithMessage").innerHTML = 'Chat with ' + getUserName;
        document.getElementById("imgProfileChatMessage").setAttribute('src', imgpath);
        document.getElementById("onlineChatMessageStatus").className = classOnlineName;
        @*document.getElementById("totalMessageChat").innerHTML = @totalMessage + ' Messages';*@

        currentChatWithName = getUserName;
        LoadMessage(getUserName);
    }

}
function AddBadgesSpan(count) {

    var conferenceMenu = document.getElementById("ConferenceMenu");
    var getCurrentSpan = conferenceMenu.getElementsByClassName("badge");

    if (getCurrentSpan.length > 0) {
        count += Number(getCurrentSpan[0].innerHTML);
        getCurrentSpan[0].innerHTML = count;
    }
    else {

        var newSpan = document.createElement("SPAN");
        newSpan.className = "badge badge-danger";
        newSpan.innerHTML = count;
        conferenceMenu.appendChild(newSpan)
    }
    }
</script>

